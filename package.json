{
  "name": "paynamics-method",
  "version": "1.0.0",
  "description": "Paynamics payment gateway integration for Snipcart; from SleekyPay custom gateway repo",
  "scripts": {
    "website:dev": "parcel ./src/*.html ./src/assets/*",
    "website:build": "parcel build ./src/*.html ./src/assets/*",
    "functions:dev": "concurrently --kill-others \"parcel ./src/functions/*.ts --out-dir dist/functions --target node\" \"parcel ./src/functions/bin/*.ts --out-dir dist/functions/bin --target node\"",
    "functions:build": "parcel build ./src/functions/*.ts --out-dir dist/functions --target node && parcel build ./src/functions/bin/*.ts --out-dir dist/functions/bin --target node",
    "build": "npm run website:build && npm run functions:build",
    "dev": "concurrently --kill-others \"npm run website:dev\" \"npm run functions:dev\"",
    "start": "netlify dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espidesigns/paynamics-method.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/espidesigns/paynamics-method/issues"
  },
  "homepage": "https://github.com/espidesigns/paynamics-method#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.40",
    "@types/uuid": "^3.4.7",
    "@types/base-64": "^1.0.0",
    "concurrently": "^5.1.0",
    "netlify": "^3.0.0",
    "parcel-bundler": "^1.8.1",
    "sass": "^1.25.0",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "base-64": "^1.0.0",
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "short-uuid": "^4.2.2",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "bindings": "^1.5.0",
    "cleave.js": "^1.5.3",
    "node-fetch": "^2.6.0",
    "react-router-dom": "^6.8.2"
  }
}
